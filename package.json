{
  "name": "@kineticdata/react",
  "version": "5.0.8-alpha.0",
  "description": "A React library for the Kinetic Platform",
  "main": "lib/index.js",
  "main:src": "src/index.js",
  "files": [
    "assets",
    "lib"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production babel src --out-dir lib",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .js",
    "preversion": "yarn lint",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "deepequal": "^0.0.1",
    "draft-js": "^0.11.0",
    "history": "^4.9.0",
    "immutable": "4.0.0-rc.9",
    "jest-immutable-matchers": "^2.0.1",
    "kld-intersections": "^0.7.0",
    "lodash": "^4.17.15",
    "lodash-es": "^4.17.15",
    "lodash.isplainobject": "^4.0.6",
    "lodash.isstring": "^4.0.1",
    "modularscale-sass": "^3.0.8",
    "moment": "^2.21.0",
    "prismjs": "^1.17.1",
    "prop-types": "^15.7.2",
    "react-autosuggest": "^9.4.3",
    "react-avatar": "^3.5.0",
    "react-dropzone": "^4.2.3",
    "react-markdown": "^4.0.6",
    "react-popper": "^1.3.4",
    "react-redux": "^6.0.0",
    "reactstrap": "^7.1.0",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.1"
  },
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@react-workspaces/react-scripts": "^3.4.0-alpha-01",
    "@types/jest": "^25.2.1",
    "babel-plugin-prismjs": "^1.1.1",
    "babel-polyfill": "^6.26.0",
    "cross-env": "^5.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "^3.3.5",
    "faker": "^4.1.0",
    "jest-websocket-mock": "^2.0.2",
    "mock-socket": "^9.0.3",
    "react-addons-test-utils": "^15.6.2",
    "stylelint": "^10.1.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-order": "^3.0.1",
    "wait-for-expect": "^3.0.2"
  },
  "author": "Kinetic Data, Inc",
  "homepage": "https://kineticdata.github.io/react-kinetic-lib",
  "license": "MIT",
  "repository": "https://github.com/kineticdata/react-kinetic-lib",
  "keywords": [
    "react-component",
    "kinetic data",
    "kinetic",
    "kineticdata",
    "react",
    "bundle"
  ],
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "absoluteRuntime": false
        }
      ]
    ],
    "plugins": [
      [
        "prismjs",
        {
          "languages": [
            "javascript",
            "ruby"
          ]
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
